{
  "name": "Twitter StickyTabs",
  "version": "2.0",
  "manifest_version": 2,
  
  "browser_action": {
      "default_icon": "images/icon.png",
      "default_title": "Click to activate StickyTabs"
   },

  "description": "Keeps the current tab selected when opening links from your Twitter Feed.",
  
  "icons": { "16": "images/16.png",
             "48": "images/48.png",
            "128": "images/128.png" },
  
  "background": {
  "scripts": [
  	"background.js"
  ]
  },
 
  
 
  "permissions": [
  "http://*.twitter.com/*",
  "https://*.twitter.com/*",
  "tabs"
  
  ]

}